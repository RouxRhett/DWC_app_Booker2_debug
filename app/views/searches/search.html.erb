<div class='container px-5 px-sm-0'>
  <div class='row'>
  <% if @model == 'user' %>
    <h3>Users search for "<%= @content %>"</h3>
    <%= render partial: 'users/index', locals: { users: @records } %>
  <% else @model == 'posts' %>
    <h3>Books search for "<%= @content %>"</h3>
    <%= render partial: 'books/index', locals: { books: @records } %>
  <% end %>
  </div>
</div>
